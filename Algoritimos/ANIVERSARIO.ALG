algoritmo "Aniversario"
var
Ano_nasc,Ano_atual,IDADE:inteiro
inicio
Escreva("Que ano estamos?")
        leia(Ano_atual)
Escreva("Que ano você nasceu?")
        leia(Ano_nasc)
IDADE<- Ano_atual-Ano_nasc
Escreva("Em ", Ano_atual ," você tera ", idade, " anos")
Se (idade>=18)entao
   escreval(" e ja tera atingido a maior idade")
senao
   escreval(" e ainda nao tem a maior idade")
 fimse
fimalgoritmo